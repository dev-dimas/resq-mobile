{
  "expo": {
    "name": "Resq Mobile",
    "slug": "resq-mobile",
    "scheme": "resq-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Aplikasi ini memerlukan izin akses foto untuk memilih profile picture atau gambar produk.",
          "cameraPermission": "Aplikasi ini memerlukan izin akses kamera untuk memotret profile picture atau gambar produk. "
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
